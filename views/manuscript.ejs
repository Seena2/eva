            <!--Content-->
            <div class="content">
                <div class="head-image">
                    <h2>MANUSCRIPT SUBMISSION FORM</h2>
                    <ul class="temp-menu">
                        <li><a class="link" href="#">Home</a></li>
                        <li><a class="link" href="#">About EVJ</a></li>
                        <li><a class="link" href="#">Explore Journals</a></li>
